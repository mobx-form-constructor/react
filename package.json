    "mobx": "^5.10.1",
    "mobx-react-lite": "^1.4.0",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2"
  },
  "peerDependencies": {
    "@mobx-form-constructor/core": "*",
    "mobx": "^4 | ^5",
    "mobx-react-lite": "^1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mobx-form-constructor/react.git"
  },
  "keywords": [
    "react",
