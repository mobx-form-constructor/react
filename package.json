{
  "name": "@mobx-form-constructor/react",
  "private": false,
  "description": "react bindings",
  "version": "0.0.1-alpha.2",
  "main": "./index.js",
  "module": "./es/index.js",
  "jsnext:main": "./es/index.js",
  "typings": "./index.d.ts",
  "author": "Alex <olefirenk@gmail.com>",
  "scripts": {
    "build:es": "cross-env tsc --target es5 --module esnext --outDir ./build/es",
